#!/bin/sh

# https://wiki.netbsd.org/ports/evbarm/qemu_arm/

QEMU_ARCH=arm
QEMU_DISK=netbsd9-armv7.qcow2
QEMU_PORT=2232
QEMU_CPUS=4
QEMU_RAMS=1G
QEMU_ENET=02:85:a1:51:a2:ef
QEMU_MCNE=virt
QEMU_BIOS=netbsd9-armv7-qemu-efi.fd
QEMU_HCPU=cortex-a15

main() {
	exec pkgvm start netbsd 9 armv7 "$@"
}

main "$@"
exit $?
